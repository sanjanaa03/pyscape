# PyScape Environment Configuration
# Copy this file to .env and add your actual API keys

# Supabase Configuration (Database & Auth)
REACT_APP_SUPABASE_URL=your-supabase-project-url
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# News API Configuration (for trending news feature)
REACT_APP_NEWS_API_KEY=your-newsapi-key-here

# RapidAPI Configuration for Judge0 Code Execution
# Get your API key from: https://rapidapi.com/judge0-official/api/judge0-ce/
REACT_APP_RAPIDAPI_KEY=your_rapidapi_key_here

# Development Configuration
NODE_ENV=development
PORT=3000
